{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "labels": {
      "team": "tbd"
    },
    "name": "spenn",
    "namespace": "default"
  },
  "spec": {
    "env": [
      {
        "name": "KAFKA_BOOTSTRAP_SERVERS",
        "value": "b27apvl00045.preprod.local:8443,b27apvl00046.preprod.local:8443,b27apvl00047.preprod.local:8443"
      },
      {
        "name": "SECURITY_TOKEN_SERVICE_REST_URL",
        "value": "http://security-token-service"
      },
      {
        "name": "SECURITYTOKENSERVICE_URL",
        "value": "https://sts-t4.test.local/SecurityTokenServiceProvider/"
      },
      {
        "name": "SIMULERING_SERVICE_URL",
        "value": "https://ztest.test.local:9128/cics/services/simulerFpServiceWSBinding"
      }
    ],
    "image": "${DOCKER_TAG}",
    "ingresses": [
      "https://spenn.nais.preprod.local"
    ],
    "liveness": {
      "failureThreshold": 10,
      "initialDelay": 20,
      "path": "/internal/isAlive",
      "periodSeconds": 5,
      "timeout": 1
    },
    "port": 8080,
    "prometheus": {
      "enabled": true,
      "path": "/internal/metrics"
    },
    "readiness": {
      "initialDelay": 20,
      "path": "/internal/isReady",
      "timeout": 1
    },
    "replicas": {
      "cpuThresholdPercentage": 90,
      "max": 1,
      "min": 1
    },
    "resources": {
      "limits": {
        "cpu": "2000m",
        "memory": "512Mi"
      },
      "requests": {
        "cpu": "1000m",
        "memory": "256Mi"
      }
    },
    "secrets": true,
    "team": "tbd"
  }
}
