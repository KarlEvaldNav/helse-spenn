<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender" >
      <encoder>
         <pattern>%date [%-5level] [%thread] %logger{5} - %m%n</pattern>
      </encoder>
   </appender>
   <logger name="org.apache.kafka.clients.NetworkClient" level="ERROR" />
   <logger name="no.nav" level="DEBUG" />
   <logger name="org.jooq" level="INFO" />
   <!-- remember to remove this when we go prod  -->
   <logger name="org.springframework.jms.listener.DefaultMessageListenerContainer" level="OFF" />
   <root level="INFO">
      <appender-ref ref="STDOUT"/>
   </root>
</configuration>
